# Site settings
title: GT New Horizons Guide
description: Comprehensive guide for GT New Horizons
baseurl: "/gtnhnotes"
url: "" # Your GitHub Pages URL will be username.github.io

# Build settings
markdown: kramdown
kramdown:
  input: GFM
  syntax_highlighter: rouge

# Theme settings
remote_theme: just-the-docs/just-the-docs

# Navigation
aux_links:
  "Guide on GitHub":
    - "//github.com/lukejameson/gtnhnotes" # Change to your repository URL

# Just the Docs settings
search_enabled: true
heading_anchors: true

collections:
  guides:
    output: true
    permalink: /:collection/:path/

defaults:
  - scope:
      path: ""
      type: guides
    values:
      layout: default
      nav_order: 1
